#include <iostream>
#include <vector>
#include <algorithm>
#include <string>
using namespace std;


int main(void) {
	int A = 300;
	int B = 60;
	int C = 10;
	int T = 0;
	vector<int> v;
	cin >> T; // 전자레인지 돌릴 초 선택

	if ((T % C) != 0) { // 10으로 다 나눌수 없다면 -1 출력하고 종료하자
		cout << -1;
		return 0;
	}


	if (T / A) {
		v.push_back(T / A);
		T %= A;
	}
	else {
		v.push_back(0);
	}

	if (T / B) {
		v.push_back(T / B);
		T %= B;
	}
	else {
		v.push_back(0);
	}

	if (T / C) {
		v.push_back(T / C);
		T %= C;
	}
	else {
		v.push_back(0);
	}
	
	for (int i = 0; i< v.size(); i++) {
		cout << v[i] << " ";
	}

	return 0;
}
